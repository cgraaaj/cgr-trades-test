DROP TABLE IF EXISTS options.ticker;

CREATE TABLE IF NOT EXISTS options.ticker
(
    id uuid NOT NULL,
    stock_id uuid,
    symbol_token integer,
    expiry_date date,
    strike_price numeric,
    option_type character varying,
    PRIMARY KEY (id)
);

DROP TABLE IF EXISTS options.candle_stick;

CREATE TABLE IF NOT EXISTS options.candle_stick
(
    id uuid NOT NULL,
    ticker_id uuid,
    time_stamp timestamp without time zone,
    open integer,
    high integer,
    low integer,
    close integer,
    volume bigint,
    open_interest integer,
    PRIMARY KEY (id)
);

DROP TABLE IF EXISTS options.stock;

CREATE TABLE IF NOT EXISTS options.stock
(
    id uuid NOT NULL,
    name character varying,
    nifty_fifty_index integer,
    PRIMARY KEY (id)
);

ALTER TABLE IF EXISTS options.ticker
    ADD CONSTRAINT stock_id_fkey FOREIGN KEY (stock_id)
    REFERENCES options.stock (id) MATCH SIMPLE
    ON UPDATE NO ACTION
    ON DELETE NO ACTION
    NOT VALID;


ALTER TABLE IF EXISTS options.candle_stick
